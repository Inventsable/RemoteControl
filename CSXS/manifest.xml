<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="com.remotetest" ExtensionBundleVersion="1.0.0" ExtensionBundleName="remotetest" Version="8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.remotetest.panel" Version="1.0.0" />
    <Extension Id="com.remotetest.modal" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList><Host Name="ILST" Version="[23,99.9]" />
      <Host Name="AEFT" Version="[16,99.9]" /></HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="8.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.remotetest.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./src/index-dev.html</MainPath>
          <!-- <MainPath>./dist/spa/index.html</MainPath> -->
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>RemoteTest</Menu>
          <Geometry>
            <Size>
              <Height>400</Height>
              <Width>280</Width>
            </Size>
            <MaxSize>
              <Height>500</Height>
              <Width>598</Width>
            </MaxSize>
            <MinSize>
              <Height>300</Height>
              <Width>260</Width>
            </MinSize>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
    <Extension Id="com.remotetest.modal">
      <DispatchInfo>
        <Resources>
          <MainPath>./src/index-dev.html</MainPath>
          <!-- <MainPath>./dist/spa/index.html</MainPath> -->
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>ModalDialog</Type>
          <Geometry>
            <Size>
              <Height>500</Height>
              <Width>600</Width>
            </Size>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
